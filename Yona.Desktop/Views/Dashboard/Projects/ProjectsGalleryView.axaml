<UserControl
    x:Class="Yona.Desktop.Views.Dashboard.Projects.ProjectsGalleryView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cc="using:Yona.Desktop.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="using:Yona.Core.ViewModels.Dashboard.Projects"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:DataType="vm:ProjectsGalleryViewModel"
    mc:Ignorable="d">
    <cc:Page Title="Projects">
        <cc:ProjectsGallery Command="{Binding OpenProjectCommand}" ProjectsSource="{Binding Projects}" />
    </cc:Page>
</UserControl>
